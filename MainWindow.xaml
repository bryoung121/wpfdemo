<Window x:Class="wpfFileDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpfFileDemo"
        mc:Ignorable="d"
        Title="WPF Demo" Height="450" Width="800">
    <Grid>
        <TextBox Name="tbLogFile" AcceptsReturn="True" TextWrapping="Wrap" IsReadOnly="True" Margin="10,54,10,10" />
        <Button Content="Move File Test" HorizontalAlignment="Left" Margin="10,21,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <Button Content="Open Power Point" HorizontalAlignment="Left" Margin="635,21,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>
        <Button Content="File Received Test" HorizontalAlignment="Left" Margin="109,21,0,0" VerticalAlignment="Top" Click="Btn_FileReceived"/>
    </Grid>
</Window>
